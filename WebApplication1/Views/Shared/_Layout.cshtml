<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="~/Content/css/main1.css">
    <script src="https://kit.fontawesome.com/a076d05399.js" crossorigin="anonymous"></script>
    <link href="//fonts.googleapis.com/css?family=Roboto:100,100i,300,300i,400,400i,500,500i,700,700i,900,900i" rel="stylesheet">
    <link href="//fonts.googleapis.com/css?family=Montserrat:100,100i,200,200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i"
          rel="stylesheet">
    <link href="~/css/style.css" rel="stylesheet" />
    <link href="~/css/style-signup.css" rel="stylesheet" />
    <title>@ViewBag.Title - My ASP.NET Application</title>

    @Styles.Render("~/Content/bootstrapMinty.css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <nav class="navbar navbar-fixed-top navbar-expand-lg navbar-light bg-light">
        <div class="container">
            <a class="navbar-brand col-md-4" href="~/Home/Index">FIND HOME</a>

            <div class="collapse navbar-collapse " id="navbarColor03">
                <ul class="navbar-nav me-auto">
                    <li class="nav-item">
                        <a class="nav-link active" href="~/Home/Index">
                            Home
                            <span class="visually-hidden">(current)</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="~/USERs/Index">Features</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="~/Home/Contact">Contact US</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="~/Home/About">About US</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="~/DEVELOPERs/Index">Developer</a>
                    </li>


                    @if (Session["DeveloperEmail"] != null)
                    {

                        <li class="nav-item">
                            <a class="nav-link" href="#">@Session["DeveloperEmail"]</a>
                        </li>
                        <li class="nav-item">

                            @Html.ActionLink("Logout", "SignOut", "User")
                        </li>



                    }
                    else
                    {
                        <li class="nav-item">
                            <a class="nav-link " href="~/DeveloperLogin/DeveloperLogin">Developer Login</a>

                        </li>
                        <li class="nav-item">
                            <a class="nav-link " href="~/DEVELOPERs/Create">Developer SignUp</a>

                        </li>


                    }

                    @if (Session["UserEmail"] != null)
                    {

                        <li class="nav-item">
                            <a class="nav-link" href="#">@Session["UserEmail"]</a>
                        </li>
                        <li class="nav-item">

                            @Html.ActionLink("Logout", "SignOut", "User")
                        </li>



                    }
                    else
                    {
                        <li class="nav-item">
                            <a class="nav-link " href="~/User/Login">LogIn</a>

                        </li>
                        <li class="nav-item">
                            <a class="nav-link " href="~/User/Registration">SignUp</a>

                        </li>


                    }


                </ul>
                
               
            </div>
        </div>
    </nav>





    <div class="container body-content text-center">
        @RenderBody()
        <hr />
        <footer>
            <p class="footer1 text-white">&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
